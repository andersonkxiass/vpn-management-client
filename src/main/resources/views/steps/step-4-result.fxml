<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane styleClass="stepPane"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.futurice.intra.vpn.view.WizardStep4ResultController">
    <top>
        <Label styleClass="header" text="Please read text below. Step 4/4."></Label>
    </top>

    <center>
        <VBox alignment="TOP_CENTER">
            <Label fx:id="resultText" text="" wrapText="true"></Label>
        </VBox>
    </center>

    <bottom>
        <HBox styleClass="navButtons">
            <Button fx:id="prevButton" mnemonicParsing="false" onAction="#buttonPrevOnAction" text="Prev" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#buttonNextOnAction" text="Close" />
        </HBox>
    </bottom>
</BorderPane>
